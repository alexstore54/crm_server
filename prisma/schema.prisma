generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Log {
  id        String   @id @default(uuid())
  level     LogLevel @default(INFO)
  message   String
  context   Json?
  createdAt DateTime @default(now())
}

enum LogLevel {
  INFO
  WARN
  ERROR
}
